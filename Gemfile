# Gemfile

# Source for Ruby gems
source "https://rubygems.org"

# Core Jekyll functionality
gem 'jekyll', '~> 4.3'
gem 'wdm', '>= 0.1.0' if Gem.win_platform?

# Group for Jekyll development plugins
group :jekyll_plugins do
  gem 'webrick'
  gem 'jekyll-sitemap'
  gem 'jekyll-seo-tag'
  gem 'jekyll-feed'
  gem 'jekyll-gzip'
end
